
<!doctype html>
<html lang="<%=lang%>">
<%-include("web/partials/head")%>
<body>
	<%-include("web/partials/header")%>
	<%-include("web/partials/nav")%>
	<main>
		<div class="alerts">
			<div class="alert alert-success d-none">
				<img src="/public/img/png/check.png" class="alert-icon d-none-phone">
				<p class="alert-text"><%=msgs.getMsgOk()%></p>
				<span class="alert-close">&times;</span>
			</div>
			<div class="alert alert-info d-none">
				<img src="/public/img/png/info-about.png" class="alert-icon d-none-phone">
				<p class="alert-text"><%=msgs.getMsgInfo()%></p>
				<span class="alert-close">&times;</span>
			</div>
			<div class="alert alert-warn d-none">
				<img src="/public/img/png/warning2.png" class="alert-icon d-none-phone">
				<p class="alert-text"><%=msgs.getMsgWarn()%></p>
				<span class="alert-close">&times;</span>
			</div>
			<div class="alert alert-danger d-none">
				<img src="/public/img/png/error.png" class="alert-icon d-none-phone">
				<p class="alert-text"><%=msgs.getMsgError()%></p>
				<span class="alert-close">&times;</span>
			</div>
		</div>
		<%-include(_tplBody)%>
	</main>
	<%-include("web/partials/footer")%>
</body>
</html>
