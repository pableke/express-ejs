<div>
	<h1>This is great</h1>
	<p><%=i18n["lblBienvenido"]%></p>
</div>

<div class="">
	<h2>Form</h2>
	<hr/>

	<form id="test" action="/test.html?lang=en" method="post" class="ajax">
		<div class="input-group">
			<label for="nombre" class="required">Name:</label>
			<input type="text" id="nombre" name="nombre" value="" size="20" maxlength="200" class="input-item input-control " tabindex="1" title="User name" placeholder="Joe" />
			<i class="input-item input-icon fas fa-signature"></i>
			<span class="invalid-feedback"></span>
		</div>

		<div class="input-group">
			<label for="correo" class="required">E-Mail</label>
			<input type="text" id="correo" name="correo" value="" size="40" maxlength="200" tabindex="2" class="input-item input-control clearable" title="Type your email" placeholder="xxxx@yyyy.com" />
			<i class="input-item input-icon far fa-envelope"></i>
			<div class="invalid-feedback"></div>
		</div>

		<div class="input-group">
			<label for="ac-user" class="required">jQuery Auto Suggest Textbox with Bootstrap 4</label>
			<input type="text" id="ac-user" value="" size="40" tabindex="3" class="input-item input-control clearable ac-user" placeholder="Search..." title="Autocomplete country textbox" />
			<input type="hidden" id="idUsuario" name="idUsuario" value="" />
			<i class="input-item input-icon fa fa-search"></i>
		</div>

		<div class="form-group">
			<label for="date" class="required">Date:</label>
			<div class="input-group">
				<input type="text" id="date" name="date" value="" size="40" maxlength="10" tabindex="3" 
					class="form-control datepicker" placeholder="format-lang" />
				<div class="input-group-append"><span class="input-group-text" id="basic-addon2"><i class="fa fa-calendar"></i></span></div>
				<div class="invalid-feedback"></div>
			</div>
			<small>ej: @ejDate;</small>
		</div>

		<div class="form-group">
			<label for="time">Time:</label>
			<div class="input-group">
				<input id="time" type="text" value="" tabindex="4" class="form-control date datepicker" />
				<i class="input-icon far fa-clock"></i>
			</div>
		</div>

		<div class="form-group">
			<label for="f1">Date Range:</label>
			<div class="input-group">
				<input type="text" id="f1" name="f1" class="form-control" value="2012-04-05" tabindex="5" />
				<div class="input-group-append"><span class="input-group-text" id="basic-addon2">...</span></div>
				<input type="text" id="f2" name="f2" class="form-control" value="2012-04-19" tabindex="6" />
			</div>
		</div>

		<div class="input-group">
			<label for="asunto" class="required">Subject:</label>
			<input type="text" id="asunto" name="asunto" value="" size="40" maxlength="200" class="input-item input-control" tabindex="3" placeholder="Type a resume" />
			<span class="invalid-feedback"></span>
		</div>

		<div class="form-group col-md-4">
			<label for="info" class="required">Comentarios:</label>
			<textarea id="info" name="info" class="form-control" tabindex="7" cols="62" rows="6" maxlength="600"></textarea>
			<span class="invalid-feedback"></span>
			<p>NÂº de caracteres restantes = <span id="counter-info">600</span></p>
		</div>

		<hr/>
		<button type="submit" class="btn btn-primary" tabindex="100">Send &raquo;</button>
		<button type="reset" class="btn btn-light ml-2" tabindex="101">&olarr; Clear</button>

		<!-- Example single danger button -->
		<button type="button" class="btn btn-danger dropdown" tabindex="102">Action &dtrif;</button>
		<div class="dropdown-menu">
			<a class="dropdown-item" href="#">Action</a>
			<a class="dropdown-item" href="#">Another action</a>
			<a class="dropdown-item" href="#">Something else here</a>
			<div class="dropdown-divider"></div>
			<a class="dropdown-item" href="#">Separated link</a>
		</div>

		<button href="contact.html?number=14,63" class="ajax-submit btn btn-success ml-2" tabindex="103">Ajax Send &raquo;</button>
		<button href="contact.html?number=14,63" class="ajax-submit btn btn-warn ml-2" tabindex="103">Warning &raquo;</button>
		<button href="contact.html?number=14,63" class="ajax-submit btn btn-dark ml-2" tabindex="103">Dark &raquo;</button>
	</form>
</div>
