
<div class="wrapper">
	<h1><%=i18n.msgFormLogin%></h1>
	<hr/>

	<form id="pass" method="post" action="/user/pass.html">
		<div class="input-group">
			<label for="oldPass" class="required"><%=i18n.lblClaveAct%>:</label>
			<input type="password" id="oldPass" name="oldPass" value="" size="35" maxlength="200" class="input-item input-control" tabindex="1" title="@titUsuario;" placeholder="********" />
			<i class="input-item input-icon fas fa-key"></i>
			<span class="msg-error"><%=msgs.oldPass%></span>
		</div>
		<div class="input-group">
			<label for="clave" class="required"><%=i18n.lblNewClave%>:</label>
			<input type="password" id="clave" name="clave" value="" size="35" maxlength="200" class="input-item input-control" tabindex="2" title="@titClave;" placeholder="********" />
			<i class="input-item input-icon fas fa-ellipsis-h"></i>
			<span class="msg-error"><%=msgs.clave%></span>
		</div>
		<div class="input-group">
			<label for="reclave" class="required"><%=i18n.lblReClave%>:</label>
			<input type="password" id="reclave" name="reclave" value="" size="35" maxlength="200" class="input-item input-control" tabindex="3" title="@titClave;" placeholder="********" />
			<i class="input-item input-icon fas fa-ellipsis-h"></i>
			<span class="msg-error"><%=msgs.reclave%></span>
		</div>

		<hr/>
		<a href="/user/index.html" tabindex="100" class="btn btn-primary">&#8810; <%=i18n.lblVolver%></a>
		<button type="submit" class="btn btn-success" tabindex="101" title="@titAcceder;"><%=i18n.lblGuardar%> <i class="far fa-save"></i></button>
		<button type="reset" class="btn btn-light" tabindex="102" title="@titRestaurar;">&olarr; <%=i18n.lblLimpiar%></button>
	</form>
</div>
